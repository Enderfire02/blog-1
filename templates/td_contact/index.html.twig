{% extends 'base.html.twig' %}

{% block title %}Hello TdContactController!{% endblock %}

{% block body %}


<div class="example-wrapper">
    <h1>TD symfony (╯°□°）╯︵ ┻━┻</h1>
{{ form(form) }}


    <h2>Mes demandes de contact ({{tdContact | length}})</h2>


    {% for tdcontact in tdContact %}
        <a href="{{path('td_contact_User', {id: tdcontact.id}) }}">
            Demande de contact de {{tdcontact.firstname}} {{tdcontact.name}}
        </a>
        </br>
    {% endfor %}


</div>
{% endblock %}
